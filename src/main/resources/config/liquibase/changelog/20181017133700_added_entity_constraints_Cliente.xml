<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20181017133700-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="fatura_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_fatura_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fatura"/>

        <addForeignKeyConstraint baseColumnNames="clientes_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_clientes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria_cliente"/>

    </changeSet>
</databaseChangeLog>
